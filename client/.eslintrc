{
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": ["prettier", "jsx-a11y"],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules":{
    "linebreak-style": 0,
    "react/prop-types": 0,
    "react/jsx-one-expression-per-line": 0
  }
}